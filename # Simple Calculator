num1 = int(input('Enter a number '))
num2 = int(input('Enter a number '))
opera = input('Enter an operator ')

if opera == '+':
    print(num1 + num2)

elif opera == '-':
    print(num1 - num2)

elif opera == '*':
    print(num1 * num2)

elif opera == '/':
    if num2 == 0:
        print('Error')
    else:
        print(num1 / num2)
